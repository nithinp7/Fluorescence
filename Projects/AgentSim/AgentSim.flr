# DECLARATIONS

struct Agent {
  vec3 position;
  float radius;
}
struct_size: 16

uint agentCount: 256

structured_buffer agentBuffer: Agent agentCount

compute_shader CS_MoveAgents: 32 1 1

# TASKS

compute_dispatch: CS_MoveAgents agentCount 1 1

barrier: agentBuffer

display_pass: VS_AgentSimDisplay PS_UvTest
